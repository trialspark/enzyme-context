{
  "name": "enzyme-context-apollo-3",
  "version": "0.0.0",
  "keywords": [
    "enzyme",
    "react",
    "context",
    "plugins",
    "apollo",
    "testing",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc -b .",
